[
    {
        "id": 1,
        "priority": 1,
        "action": {
            "type": "modifyHeaders",
            "responseHeaders": [
                {
                    "header": "Access-Control-Allow-Origin",
                    "operation": "set",
                    "value": "*"
                },
                {
                    "header": "Access-Control-Allow-Methods",
                    "operation": "set",
                    "value": "GET, PUT, POST, DELETE, HEAD, OPTIONS"
                }
            ]
        },
        "condition": {
            "urlFilter": "*",
            "resourceTypes": [
                "media",
                "xmlhttprequest",
                "other"
            ],
            "excludedInitiatorDomains": [
                "youtube.com",
                "www.youtube.com",
                "doubleclick.net",
                "googleads.g.doubleclick.net"
            ],
            "excludedRequestDomains": [
                "youtube.com",
                "googlevideo.com",
                "doubleclick.net",
                "googleads.g.doubleclick.net",
                "google.com",
                "googleapis.com"
            ]
        }
    }
]