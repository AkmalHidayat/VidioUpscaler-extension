{
  "manifest_version": 3,
  "name": "Anime4K Web Upscaler",
  "version": "2.6.4",
  "description": "Real-time video upscaling with Anime4K, FSR, and more",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "Model/debug.js",
        "Model/anime4k_fast.js",
        "Model/anime4k_hq.js",
        "Model/fsr.js",
        "Model/xbrz.js",
        "Model/cas.js",
        "Model/bicubic.js",
        "Model/realsr.js",
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "net_request_rules.json"
      }
    ]
  }
}